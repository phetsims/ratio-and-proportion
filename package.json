{
  "name": "ratio-and-proportion",
  "version": "1.3.0-dev.2",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/ratio-and-proportion.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "RATIO_AND_PROPORTION",
    "phetLibs": [
      "griddle",
      "tangible"
    ],
    "preload": [
      "../sherpa/lib/mediaPipe/hands@0.4.1646424915/hands.js"
    ],
    "simulation": true,
    "runnable": true,
    "generatedUnitTests": true,
    "simFeatures": {
      "supportsSound": true,
      "supportsExtraSound": true,
      "supportsInteractiveDescription": true,
      "supportsVoicing": true,
      "supportsDynamicLocale": true
    },
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "supportsOutputJS": true,
    "published": true,
    "screenNameKeys": [
      "RATIO_AND_PROPORTION/screen.discover",
      "RATIO_AND_PROPORTION/screen.create"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "parserOptions": {
          "project": [
            "../chipper/tsconfig/eslint-all/tsconfig.json"
          ]
        }
      }
    ]
  }
}