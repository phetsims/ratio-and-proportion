    "discover": {
      "homeScreenDescription": {
        "value": "Explore this screen to discover set challenge ratios."
      },
      "screenSummary": {
        "paragraph1": {
          "value": "The Play Area is a tall space containing a Left Hand and a Right Hand that can be moved up and down. You can move the Left Hand and the Right Hand individually, or activate Both Hands to move both hands at the same time. As you move the hands up and down, you will get closer to or farther from the Challenge Ratio."
        },
        "paragraph2": {
          "value": " There are options to add plain or numbered tick marks. The tick marks are evenly-spaced horizontal lines that divide up the vertical space. When ready, you can choose a different challenge to master."
        },
        "interactionHint": {
          "value": "Move hands to find the Challenge Ratio in different places."
        },
        "qualitativeStateOfSim": {
          "value": "Currently, hands are {{ratioFitness}} the {{currentChallenge}} ratio and {{distance}} each other:"
        }
      },
      "challengesHelpText": {
        "value": "Choose a challenge, then move hands to explore it."
      },
      "challenge1Lowercase": {
        "value": "challenge 1"
      },
      "challenge2Lowercase": {
        "value": "challenge 2"
      },
      "challenge3Lowercase": {
        "value": "challenge 3"
      },
      "bothHandsGestureDescriptionHelpText": {
        "value": "Explore challenge ratio with individual Left and Right hands. The custom Both Hands interaction is not yet possible with screen reader software on a mobile device."
      }
    },
    "create": {
      "homeScreenDescription": {
        "value": "On this screen, set your own challenge ratios to explore."
      },
      "screenSummary": {
        "paragraph1": {
          "value": "Here, you can create your own Challenge Ratio. The Play Area is a tall space containing a Left Hand and a Right Hand that can be moved up and down. You can move the Left Hand and the Right Hand individually, or activate Both Hands to move both hands at the same time. As you move the hands up and down, you will get closer to or farther from the Challenge Ratio, unless you lock the ratio."
        },
        "paragraph2": {
          "value": "There are options to add plain or numbered tick marks. The tick marks are evenly-spaced horizontal lines that divide up the vertical space. In addition, you can increase the range of tick marks to explore larger numbers."
        },
        "interactionHint": {
          "value": "Explore current challenge, or look for My Challenge to create your own."
        },
        "qualitativeStateOfSim": {
          "value": "Currently, hands {{ratioFitness}} challenge ratio and {{distance}} each other:"
        }
      },
      "tickMarkRangeHelpText": {
        "value": "When tick marks shown, choose range for tick marks."
      },
      "tickMarkRangeContextResponse": {
        "value": "Tick mark range now {{range}}."
      },
      "myChallengeHelpText": {
        "value": "Set left and right values to create your challenge, then move hands to explore it."
      },
      "bothHandsGestureDescriptionHelpText": {
        "value": "Explore challenge ratio with individual Left and Right hands, or try with ratio locked. The custom Both Hands interaction is not yet possible with screen reader software on a mobile device."
      }
    },
